{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch . --ext js --exec node --experimental-json-modules server.js",
    "build": "npm install && npx prisma generate && npx prisma migrate deploy",
    "fix:status-column": "node scripts/fix-status-column.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:push": "npx prisma db push --accept-data-loss",
    "prisma:resolve-applied": "npx prisma migrate resolve --applied 20250812101902_make_updatedat_optional",
    "prisma:resolve-rolledback": "npx prisma migrate resolve --rolled-back 20250812101902_make_updatedat_optional",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.js\"",
    "docker:build": "docker build -t wingo-backend .",
    "docker:run": "docker run -p 5000:5000 --env-file .env wingo-backend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/lib-storage": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@prisma/client": "^6.14.0",
    "aws-sdk": "^2.1532.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "razorpay": "^2.9.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "form-data": "^4.0.4",
    "localtunnel": "^2.0.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0"
  }
}
