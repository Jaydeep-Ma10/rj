# Server Configuration
PORT=10000
PUBLIC_URL="http://localhost:10000"
NODE_ENV="development"

# JWT Configuration (REQUIRED - Generate secure secrets)
JWT_SECRET="your-super-secure-jwt-secret-here-min-32-chars"
JWT_REFRESH_SECRET="your-super-secure-refresh-secret-here-min-32-chars"

# AWS S3 Configuration (Optional - will fallback to local storage if not configured)
AWS_ACCESS_KEY_ID="your-aws-access-key-id"
AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"
AWS_REGION="us-east-1"
AWS_S3_BUCKET_NAME="your-s3-bucket-name"

# SMS Provider Configuration (Optional - uses mock SMS if not configured)
SMS_PROVIDER="mock"
SMS_API_KEY="your-sms-api-key"
SMS_API_URL="https://api.sms-provider.com/send"

# Admin Configuration
